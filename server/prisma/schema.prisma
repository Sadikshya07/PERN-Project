// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String @id @default(uuid())
  name     String
  password String
}

model courses {
  id   String @id @default(uuid())
  name String
  description String
}

model dsscourses {
  id          String @id @default(uuid())
  name        String
  description String
}

model dssclubs {
  id          String @id @default(uuid())
  name        String
  description String
}

model faculty {
  id          String @id @default(uuid())
  name        String
  description String
  department  String
}

model schoolactivities {
  id    String @id @default(uuid())
  Title String
}

model newsandevents {
  id          String   @id @default(uuid())
  title       String
  description String
  author      String
  publishdate DateTime
}

model management {
  id          String @id @default(uuid())
  name        String
  description String
  position    String
}

model schoolinmedia {
  id     String @id @default(uuid())
  title  String
  author String
}

model weekendcampprogram {
  id          String @id @default(uuid())
  description String
}

model whydss {
  id       String @id @default(uuid())
  bullets  String
  category String
}

model newsletter {
  id   String @id @default(uuid())
  name String
}

model termsummary {
  id   String @id @default(uuid())
  name String
}

model deerwalker {
  id   String @id @default(uuid())
  name String
}

model analysisreport {
  id   String @id @default(uuid())
  name String
}

model studentcorner {
  id             Int    @id @default(autoincrement())
  studentname    String
  rollnumber     Int
  grade          Int
  articletitle   String
  articlecontent String
}

model podcast {
  id            Int    @id @default(autoincrement())
  title         String
  presentername String
  rollnumber    Int
  grade         Int
  description   String
}
