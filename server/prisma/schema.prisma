generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String @id @default(uuid())
  name     String
  password String
}

//home
model addheroimages {
  id String @id @default(uuid())
  Page String
  Image String
}

model metrics {
  id String @id @default(uuid())
  Students String
  StudentsPerClass String
  Teachers String
  StudentTeacherRatio String
}

model programs {
  id String @id @default(uuid())
  Image String
}

model schoolactivities {
  id    String @id @default(uuid())
  Link String
}

model schoolinmedia {
  id     String @id @default(uuid())
  title  String
  author String
  Image String
  Link String
}

//about us

model faculty {
  id          String @id @default(uuid())
  name        String
  description String
  department  String
  AreaofExpertise String
  Experience String
}

model management {
  id          String @id @default(uuid())
  name        String
  description String
  position    String
}

model whydss {
  id       String @id @default(uuid())
  bullets  String
  category String
}

//programs

model courses {
  id          String @id @default(uuid())
  name        String
  description String
}

model dsscourses {
  id          String @id @default(uuid())
  name        String
  description String
}

model weekendcampprogram {
  id          String @id @default(uuid())
  description String
}


//publications

model newsletter {
  id   String @id @default(uuid())
  name String
}

model termsummary {
  id   String @id @default(uuid())
  name String
}

model deerwalker {
  id   String @id @default(uuid())
  name String
}

model analysisreport {
  id   String @id @default(uuid())
  name String
}

model studentcorner {
  id             String @id @default(uuid())
  studentname    String
  rollnumber     Int
  grade          Int
  articletitle   String
  articlecontent String
}

model podcast {
  id            String @id @default(uuid())
  title         String
  presentername String
  grade         Int
  description   String
  rollnumber    Int
}

//dss in year

model booklist {
  id   String @id @default(uuid())
  name String
}

model calendar {
  id    String @id @default(uuid())
  title String
}

model lessonplan {
  id    String @id @default(uuid())
  title String
}

//other pages

model dssclubs {
  id          String @id @default(uuid())
  name        String
  description String
}

model popup {
  id String @id @default(uuid())
  Image String
}

//community

model newsandevents {
  id          String   @id @default(uuid())
  title       String
  description String
  author      String
  publishdate DateTime @db.Date
}
