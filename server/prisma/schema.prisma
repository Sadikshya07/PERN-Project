// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String @id @default(uuid())
  name     String
  password String
}


//Homepage

model addheroimages {
  id  String @id @default(uuid())
   
}

model metrics {
  id String @id @default(uuid())
  
}

model programs {
  id  String @id @default(uuid())
  
}

model schoolactivities {
  id    String @id @default(uuid())
  Title String
}

model schoolinmedia {
  id     String @id @default(uuid())
  title  String
  author String
}


//About us

model faculty {
  id          String @id @default(uuid())
  name        String
  description String
  department  String
}

model management {
  id          String @id @default(uuid())
  name        String
  description String
  position    String
}

model whydss {
  id       String @id @default(uuid())
  bullets  String
  category String
}





//Our programs

model courses {
  id   String @id @default(uuid())
  name String
  description String
}

model dsscourses {
  id          String @id @default(uuid())
  name        String
  description String
}

model weekendcampprogram {
  id          String @id @default(uuid())
  description String
}



//Publications

model newsletter {
  id   String @id @default(uuid())
  name String
}

model termsummary {
  id   String @id @default(uuid())
  name String
}

model deerwalker {
  id   String @id @default(uuid())
  name String
}

model analysisreport {
  id   String @id @default(uuid())
  name String
}

model studentcorner {
  id             String   @id @default(uuid())
  studentname    String
  rollnumber     Int
  grade          Int
  articletitle   String
  articlecontent String
}

model podcast {
  id            String   @id @default(uuid())
  title         String
  presentername String
  rollnumber    Int
  grade         Int
  description   String
}


// Dss in year

model booklist {
  id  String @id@default(uuid())
  name String
}

model calendar{
  id String @id@default(uuid())
  title String
}

model lessonplan{
   id String @id@default(uuid())
  title String
}


//other pages

model dssclubs {
  id          String @id @default(uuid())
  name        String
  description String
}

model admission {
  id          String @id @default(uuid())

}


//community

model newsandevents {
  id          String   @id @default(uuid())
  title       String
  description String
  author      String
  publishdate DateTime @db.Date
}












