// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  name     String
  password String
}

model nebcourses {
  id   Int    @id @default(autoincrement())
  name String
}

model dsscourse {
  id          Int    @id @default(autoincrement())
  name        String
  description String
}

model dssclub {
  id          Int    @id @default(autoincrement())
  name        String
  description String
}

model faculty {
  id          Int    @id @default(autoincrement())
  name        String
  description String
}

model schoolactivities {
  id    Int    @id @default(autoincrement())
  Title String
}

model publications {
  id       Int    @id @default(autoincrement())
  Filename String
}

model studentcorner {
  id             Int    @id @default(autoincrement())
  studentname    String
  rollnumber     Int
  grade          Int
  articletitle   String
  articlecontent String
}

model podcast {
  id            Int    @id @default(autoincrement())
  title         String
  presentername String
  rollno        Int
  grade         Int
  description   String
}

model newsevents {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  author      String
  publishdate DateTime
}

model management {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  position    String
}

model schoolinmedia {
  id     Int    @id @default(autoincrement())
  title  String
  author String
}

model weekendcampname {
  id          Int    @id @default(autoincrement())
  description String
}

model whydss {
  id      Int    @id @default(autoincrement())
  bullets String
}
